<?xml version="1.0" encoding="utf-8" ?>
<PDL>
  <packet name="ServerBroadcastEnterGame">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  <packet name="ClientLeaveGame">
  </packet>
  <packet name="ServerBroadcastLeaveGame">
	<int name="playerId"/>
  </packet>
  <packet name="ServerPlayerList">
	<list name="player">
	  <bool name="isSelf"/>
	  <int name="playerId"/>
	  <float name="posX"/>
	  <float name="posY"/>
	  <float name="posZ"/>
	</list>
  </packet>
  <packet name="ClientMove">
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
  <packet name="ServerBroadcastMove">
	<int name="playerId"/>
	<float name="posX"/>
	<float name="posY"/>
	<float name="posZ"/>
  </packet>
</PDL>